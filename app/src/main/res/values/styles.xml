<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="navBtn">
        <item name="android:padding">0dp</item>
        <item name="android:paddingTop">40dp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">@color/text3</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:fontFamily">@font/medium</item>
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">54dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_marginBottom">24dp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
    </style>

    <style name="btnback">
        <item name="android:padding">4dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
    </style>

    <style name="h1">
        <item name="android:letterSpacing">0.04</item>
        <item name="android:textColor">@color/text2</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="h2">
        <item name="android:letterSpacing">0.06</item>
        <item name="android:textColor">@color/text2</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="h4">
        <item name="android:padding">0dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:letterSpacing">0.06</item>
        <item name="android:textColor">@color/text4</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="h6">
        <item name="android:padding">0dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">@color/text4</item>
        <item name="android:fontFamily">@font/medium</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="input_label">
        <item name="android:padding">0dp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">@color/text4</item>
        <item name="android:fontFamily">@font/medium</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/RootMargin</item>
        <item name="android:textSize">12sp</item>
        <item name="android:linksClickable">false</item>
        <item name="android:longClickable">false</item>
    </style>

    <style name="edit_text">
        <item name="android:paddingTop">8dp</item>
        <item name="android:layout_marginTop">-12dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">@color/text3</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">@drawable/input_bg</item>
    </style>

    <style name="btn1">
        <item name="android:padding">12dp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">@color/bg1</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/btn1_bg</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:layout_marginTop">40dp</item>
    </style>

    <style name="btn2">
        <item name="android:padding">5dp</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="android:textColor">@color/text4</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:fontFamily">@font/semibold</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="ViewBtn">
        <item name="android:padding">4dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">36dp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="greenBtn">
        <item name="android:padding">0dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
    </style>

    <style name="callIconTypes">
        <item name="android:padding">0dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">18dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_marginStart">@dimen/RootMargin</item>
        <item name="android:layout_marginTop">28dp</item>
    </style>

</resources>